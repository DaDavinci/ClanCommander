{
  "productName": "NXTBot",
  "name": "clan-commander",
  "description": "ClanCommander is a Utility Tool to add Features to Clans in RuneScape 3.\nIt features OCR and Pattern Recognition Classifiactions to handle the interfacing with the NXT Client.\nThis is basically undetectable once you add some ML Artifical Intelligence into the mix... Just so you know!",
  "authors": "DaDavinci",
  "version": "beta.2K21.05.28",
  "homepage": "https://github.com/DaDavinci/ClanCommander/blob/master/README.md",
  "license": "MIT",
  "markdown":"github",
  "keywords": [
    "RuneScape",          "Clan",             "Utility",
    "Tool",               "Screen",           "Features",
    "Management",         "looney bin riot",  "Classifiers",
    "Optical Character Recognition",          "Machine Learning",
    "Artifical Intelligence",                 "Pattern Recognition"
  ],
  "main": "dist/index.js",
  "scripts": {
    "download": "npm i --save --include=dev",
    "start": "tsc && electron-forge start",
    "package": "tsc && electron-forge package",
    "make": "tsc && electron-forge make",
    "publish": "tsc && electron-forge publish"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "tron_forge"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "win"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "1.0.0",
    "gyp": "^0.5.0",
    "nutjs": "^0.0.1",
    "tesseract.js": "^2.1.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.0-beta.50",
    "@electron-forge/maker-deb": "6.0.0-beta.50",
    "@electron-forge/maker-rpm": "6.0.0-beta.50",
    "@electron-forge/maker-squirrel": "6.0.0-beta.50",
    "@electron-forge/maker-zip": "6.0.0-beta.50",
    "commonjs": "^0.0.1",
    "devtron": "1.4.0",
    "electron": "8.0.2",
    "tsx": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DaDavinci/ClanCommander.git"
  },
  "bugs": {
    "url": "https://github.com/DaDavinci/ClanCommander/issues"
  }
}
